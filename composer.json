{
    "name": "random-str-inc/laravel-african-currencies",
    "description": "A Laravel package for African currencies and conversion.",
    "keywords": ["laravel", "currency", "africa", "conversion"],
    "homepage": "https://github.com/random-str-inc/laravel-african-currencies",
    "license": "MIT",
    "type": "laravel-package",
    "authors": [
        {
            "name": "Kamotom Hone",
            "email": "kamotomhone@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "phpoffice/phpspreadsheet": "^1.29"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "RandomStrInc\\LaravelAfricanCurrencies\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RandomStrInc\\LaravelAfricanCurrencies\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RandomStrInc\\LaravelAfricanCurrencies\\LaravelAfricanCurrenciesServiceProvider"
            ],
            "aliases": {
                "AfricanCurrency": "RandomStrInc\\LaravelAfricanCurrencies\\Facades\\AfricanCurrency"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
